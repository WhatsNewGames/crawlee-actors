{
  "name": "@wng/apify-actors",
  "packageManager": "pnpm@7.24.3",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev:all": "pnpm -r dev",
    "build:all": "pnpm -r build",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "test": "pnpm run build:all && pnpm run -r test",
    "create-actor": "esno ./scripts/create-actor.ts",
    "apify-check-health": "esno ./scripts/apify-check-health.ts",
    "post-webhook": "esno ./scripts/post-webhook.ts",
    "apify:push-all": "pnpm -r --workspace-concurrency 1 apify:push"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@types/inquirer": "^9.0.3",
    "@types/mustache": "^4.2.2",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "apify-client": "^2.6.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "esno": "^0.16.3",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "lint-staged": "^13.1.2",
    "meow": "^11.0.0",
    "mustache": "^4.2.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "undici": "^5.20.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
